{
  "battery": {
    "interval": 10,
    "states": {
      "warning": 20,
      "critical": 10,
      "plugordie": 5
    },
    "format": "{icon} {capacity}%",
    "format-icons": ["󰁺", "󰁻", "󰁼", "󰁽", "󰁾", "󰁿", "󰂀", "󰂁", "󰂂", "󰁹"],
    "format-charging": " {capacity}%",
    "format-plugged": "",
    "_comment": "Show time til battery runs out on click",
    "format-alt": "󱧥 {time}",
    "tooltip": false,
    "exec-on-event": true,
    "exec-if-event": "state-warning, state-critical"
  }
}
